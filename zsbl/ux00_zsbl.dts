		L45: pwm@10020000 {
			compatible = "sifive,pwm0";
			interrupt-parent = <&L2>;
			interrupts = <42 43 44 45>;
			reg = <0x0 0x10020000 0x0 0x1000>;
			reg-names = "control";
		};
		L46: pwm@10021000 {
			compatible = "sifive,pwm0";
			interrupt-parent = <&L2>;
