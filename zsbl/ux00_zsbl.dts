		L49: spi@10040000 {
			compatible = "sifive,spi0";
			interrupt-parent = <&L2>;
			interrupts = <51>;
			reg = <0x0 0x10040000 0x0 0x1000 0x0 0x20000000 0x0 0x10000000>;
